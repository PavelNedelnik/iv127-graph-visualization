{
    "cytoscape": {
        "position": "absolute",
        "width": "80%",
        "height": "80%",
        "z-index": 999
    },

    "stylesheet": [
        {
            "selector": "node",
            "style": {
                "content": "data(label)",
                "text-valign":"bottom",
                "text-halign":"center",
                "text-rotation": "350deg",
                "background-color": "data(color)",
                "shape": "data(shape)"
            }
        },
        {
            "selector": "edge",
            "style": {
                "target-arrow-color": "data(color)",
                "target-arrow-shape": "vee",
                "curve-style": "bezier",
                "line-color": "data(color)"
            }
        }
    ]
}